{
  "name": "gerenciador-metas-pessoais",
  "version": "1.0.0",
  "description": "Aplicativo de Gerenciamento de Metas Pessoais",
  "scripts": {
    "start": "cd server; node startup.js",
    "server": "cd server; nodemon server.js",
    "client": "cd client; npm start",
    "build": "powershell -Command \"cd client; $env:CI='false'; $env:NODE_OPTIONS='--openssl-legacy-provider'; npx react-scripts build\"",
    "install-client": "cd client; npm install",
    "install-server": "cd server; npm install",
    "install-all": "npm install; npm run install-server; npm run install-client",
    "dev": "cd server; nodemon server.js",
    "test": "cd server; npm test"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "keywords": [
    "metas",
    "gerenciamento",
    "objetivos",
    "pessoal"
  ],
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "cross-env": "^7.0.3"
  }
}
